<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/frag_noex"
    android:screenOrientation="portrait"
    android:background="@color/white"
    tools:context=".ui.Routines.exnoFragment">


    <Button
        android:id="@+id/done_button"
        android:layout_width="209dp"
        android:layout_height="70dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="118dp"
        android:layout_marginBottom="152dp"
        android:backgroundTint="#060227"
        android:text="Done"
        android:textAlignment="center"
        android:textColor="#FBF8F8"
        android:textSize="24sp"
        app:cornerRadius="20dp"
        app:iconTint="#272525" />

    <RelativeLayout
        android:id="@+id/previous_layout"
        android:layout_width="205dp"
        android:layout_height="66dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="3dp"
        android:layout_marginBottom="83dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="68dp"
            android:layout_height="46dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="8dp"
            android:clickable="true"
            android:src="@android:drawable/ic_media_previous" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="114dp"
            android:layout_height="43dp"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="2dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="8dp"
            android:layout_toEndOf="@+id/imageView8"
            android:gravity="center_vertical"
            android:text="Previous"
            android:textSize="20sp"
            android:textStyle="bold" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/skip_layout"
        android:layout_width="170dp"
        android:layout_height="67dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="83dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="114dp"
            android:layout_height="43dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="18dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="18dp"
            android:layout_marginBottom="12dp"
            android:baselineAligned="true"
            android:gravity="center_vertical"
            android:text="Skip"
            android:textAlignment="textStart"
            android:textSize="20sp"
            android:textStyle="bold"
            tools:ignore="RtlCompat" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="95dp"
            android:layout_height="46dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="-41dp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="-2dp"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="8dp"
            android:layout_toEndOf="@+id/textView2"
            android:layout_weight="1"
            android:clickable="true"
            android:rotationY="180"
            android:src="@android:drawable/ic_media_previous" />
    </RelativeLayout>

    <TextView
        android:id="@+id/ecercise_no_of_sets"
        android:layout_width="205dp"
        android:layout_height="185dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="89dp"
        android:layout_marginBottom="171dp"
        android:gravity="center_vertical"
        android:text="x10"
        android:textAlignment="center"
        android:textColor="#0E0E0E"
        android:textSize="48sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/exercise_title"
        android:layout_width="268dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="85dp"
        android:layout_marginBottom="296dp"
        android:fontFamily="serif"
        android:text="Push Up"
        android:textAlignment="center"
        android:textColor="#171616"
        android:textSize="24sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/exercise_image"
        android:layout_width="match_parent"
        android:layout_height="348dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="-2dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="2dp"
        android:background="#6474FF"
        android:src="@drawable/legraise"
        android:visibility="visible" />

    <Button
        android:id="@+id/imagebutton"
        android:layout_width="211dp"
        android:layout_height="35dp"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="0dp"
        android:backgroundTint="#FFFFFF"
        android:text="Image"
        android:textColor="#0E0E0E"
        app:cornerRadius="0dp"

        />

    <Button
        android:id="@+id/videobutton"
        android:layout_width="211dp"
        android:layout_height="35dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="202dp"
        android:layout_marginEnd="-2dp"
        android:backgroundTint="#FFFFFF"
        android:text="Video"
        android:textColor="#0E0E0E"
        app:cornerRadius="0dp" />

    <FrameLayout
        android:id="@+id/videoframe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        tools:visibility="visible">

        <VideoView
            android:id="@+id/video"
            android:layout_width="match_parent"
            android:layout_height="348dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:visibility="gone"/>

    </FrameLayout>




</RelativeLayout>